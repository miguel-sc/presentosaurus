{
  "name": "@presentosaurus/core",
  "version": "0.1.2",
  "description": "Presentosaurus webcomponents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/presentosaurus/presentosaurus.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "dev": "stencil build --dev --watch --serve",
    "start": "stencil build --watch",
    "start:storybook": "start-storybook -p 9009 -s www",
    "build": "yarn build:stencil && yarn build:storybook",
    "build:stencil": "stencil build --docs",
    "build:storybook": "build-storybook -o build -s www"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/presentosaurus/presentosaurus.git"
  },
  "devDependencies": {
    "@stencil/core": "1.8.6",
    "@storybook/addon-a11y": "5.3.0-beta.19",
    "@storybook/addon-actions": "5.3.0-beta.19",
    "@storybook/addon-backgrounds": "5.3.0-beta.19",
    "@storybook/addon-centered": "5.3.0-beta.19",
    "@storybook/addon-docs": "5.3.0-beta.19",
    "@storybook/addon-events": "5.3.0-beta.19",
    "@storybook/addon-jest": "5.3.0-beta.19",
    "@storybook/addon-knobs": "5.3.0-beta.19",
    "@storybook/addon-links": "5.3.0-beta.19",
    "@storybook/addon-notes": "5.3.0-beta.19",
    "@storybook/addon-storyshots": "5.3.0-beta.19",
    "@storybook/addon-storysource": "5.3.0-beta.19",
    "@storybook/addon-viewport": "5.3.0-beta.19",
    "@storybook/addons": "5.3.0-beta.19",
    "@storybook/client-api": "5.3.0-beta.19",
    "@storybook/core": "5.3.0-beta.19",
    "@storybook/core-events": "5.3.0-beta.19",
    "@storybook/source-loader": "5.3.0-beta.19",
    "@storybook/web-components": "5.3.0-beta.19",
    "@types/katex": "0.11.0",
    "babel-loader": "8.0.6",
    "eventemitter3": "4.0.0",
    "format-json": "1.0.3",
    "global": "4.4.0",
    "lit-element": "2.2.1",
    "material-design-icons": "3.0.1"
  },
  "dependencies": {
    "asciimath2tex": "1.1.0",
    "katex": "0.11.1",
    "prismjs": "1.21.0"
